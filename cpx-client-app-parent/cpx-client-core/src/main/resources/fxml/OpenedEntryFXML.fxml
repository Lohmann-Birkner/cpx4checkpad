<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="hboxContent" focusTraversable="true" stylesheets="@../styles/cpx-default.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.lb.cpx.client.core.menu.fx.openedEntry.OpenedEntryFXMLController">
    <children>
        <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="labelIcon">
                    <HBox.margin>
                        <Insets right="12.0" />
                    </HBox.margin>
                </Label>
                <VBox fx:id="boxContent" alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets right="8.0" />
                    </HBox.margin>
                    <children>
                  <HBox fx:id="boxTitle" spacing="2.0">
                     <children>
                              <Label fx:id="labelHeader" text="Label" />
                     </children>
                  </HBox>
                        <Label fx:id="labelDescription" text="Label" />
                    </children>
                </VBox>
                <HBox alignment="CENTER_RIGHT" HBox.hgrow="NEVER">
                    <children>
                        <Button fx:id="buttonClose" mnemonicParsing="false" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets left="24.0" />
            </padding>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="mainFxmlClass" />
        <String fx:value="opened-entry" />
    </styleClass>
</AnchorPane>
