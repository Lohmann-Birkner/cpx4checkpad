        <VBox spacing="6.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="tfSearchField" promptText="Hier Text fÃ¼r die Suche eingeben" />
                <Separator prefWidth="200.0" />
                <TreeView fx:id="tvItems" prefHeight="400.0" prefWidth="200.0" styleClass="stay-selected-tree-view" />
                <Separator prefWidth="200.0" />
                <ScrollPane fx:id="spTooltips" maxHeight="-Infinity" prefHeight="150.0">
                    <content>
                        <VBox fx:id="boxTooltips" spacing="5.0" />
                    </content>
                </ScrollPane>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</AnchorPane>
