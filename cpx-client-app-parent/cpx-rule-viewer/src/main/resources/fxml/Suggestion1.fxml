<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>

<FlowPane maxHeight="-Infinity" minHeight="-Infinity" prefWrapLength="280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox fx:id="boxContent" minWidth="-Infinity" spacing="5.0" />
        <Label fx:id="lblActionId" maxWidth="-Infinity" minWidth="-Infinity" text="Label">
            <FlowPane.margin>
                <Insets right="5.0" />
            </FlowPane.margin>
        </Label>
        <Label fx:id="lblCriterion" maxWidth="-Infinity" minWidth="-Infinity" text="Label">
            <FlowPane.margin>
                <Insets right="5.0" />
            </FlowPane.margin>
        </Label>
        <Label fx:id="lblCriterionOperator" maxWidth="-Infinity" minWidth="-Infinity" text="Label">
            <FlowPane.margin>
                <Insets right="5.0" />
            </FlowPane.margin>
        </Label>
        <Hyperlink fx:id="hlCriterionValue" styleClass="value_hyperlink" text="Hyperlink">
            <FlowPane.margin>
                <Insets />
            </FlowPane.margin>
        </Hyperlink>
        <HBox fx:id="boxCondition" alignment="CENTER">
            <children>
                <Label text="(" />
                <HBox alignment="CENTER" spacing="5.0">
                    <children>
                        <Label fx:id="lblConditionText" text="Label" />
                        <Label fx:id="lblConditionCriterion" text="Label" />
                        <Label fx:id="lblConditionOperator" text="Label" />
                        <Hyperlink fx:id="hlConditionValue" styleClass="value_hyperlink" text="Hyperlink" />
                    </children>
                </HBox>
                <Label text=")" />
            </children>
        </HBox>
    </children>
</FlowPane>
