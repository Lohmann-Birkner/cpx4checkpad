<?xml version="1.0" encoding="UTF-8"?>

<?import de.lb.cpx.client.app.cm.fx.simulation.tables.IcdTablePane?>
<?import de.lb.cpx.client.app.cm.fx.simulation.tables.OpsTablePane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane stylesheets="@../styles/cpx-default.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.lb.cpx.client.app.job.fx.casemerging.details.IcdMergeDetailsFXMLController">
    <children>
        <SplitPane fx:id="spContent" dividerPositions="0.5" layoutY="25.0" orientation="VERTICAL" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <IcdTablePane fx:id="spIcd" />
                <OpsTablePane fx:id="spOps" />
            </items>
        </SplitPane>
        <HBox fx:id="hBoxAddDiagnosisMenu" alignment="CENTER_LEFT" AnchorPane.leftAnchor="1.0" AnchorPane.topAnchor="1.0">
            <children>
                <Button fx:id="btnAddDiagnosis" mnemonicParsing="false" onAction="#onAddDiagnosis" styleClass="cpx-icon-button" />
            </children>
        </HBox>
    </children>
</AnchorPane>
